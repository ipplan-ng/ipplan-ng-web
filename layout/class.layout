class Text extends HTMLBase {
    var $classname = "text";
    var $str;
    var $fmt = "";

    function Text($s,$a = "") {
        $this->setparam($a);
        $this->str = (strlen($s)==0 and empty($s)) ? "" : htmlspecialchars($s);
    }

    function printit() {
        if ($this->settings)
           echo "<font". $this->settings . ">";
        echo str_replace("~", "", $this->fmt).$this->str.str_replace("~", "/", $this->fmt);
        if ($this->settings)
           echo "</font>";
    }
}

